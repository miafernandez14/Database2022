<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Database2022.Views.InsertPerson">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="First name" Text="{Binding FirstName}"></Entry>
            <Entry Placeholder="Last name" Text="{Binding LastName}"></Entry>
            <Button Text="Insert person" Command="{Binding InsertCommand}"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>